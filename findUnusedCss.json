{
   // paths of css files for analyzing
  "cssFiles": ["./css/*.css"],
  // path of source codes such as html or js files
  "source_files": ["./*.html", "./js/*.js"],
  // optins for analyzing
  "options" : {
    "htmlAnalyzing": true, // default true
    "tplAnalyzing": false, // default false
    "reactAnalyzing": false, // default false
    "jQueryAnalyzing": false // default false
  },
  // list of folders, which are excluded during analyzing
  "excludes": ["./node_modules"]
}
